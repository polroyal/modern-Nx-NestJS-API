    "modern-api": {
      "projectType": "application",
      "root": "apps/modern-api",
      "sourceRoot": "apps/modern-api/src",
      "prefix": "nestjsorg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/modern-api",
            "index": "apps/modern-api/src/index.html",
            "main": "apps/modern-api/src/main.ts",
            "polyfills": "apps/modern-api/src/polyfills.ts",
            "tsConfig": "apps/modern-api/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/modern-api/src/favicon.ico",
              "apps/modern-api/src/assets"
            ],
            "styles": ["apps/modern-api/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/modern-api/src/environments/environment.ts",
                  "with": "apps/modern-api/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "modern-api:build:production"
            },
            "development": {
              "browserTarget": "modern-api:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/modern-api/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "modern-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/modern-api/src/**/*.ts",
              "apps/modern-api/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/modern-api"],
          "options": {
            "jestConfig": "apps/modern-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "modern-api-e2e": {
      "root": "apps/modern-api-e2e",
      "sourceRoot": "apps/modern-api-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/modern-api-e2e/cypress.json",
            "devServerTarget": "modern-api:serve:development",
            "tsConfig": "apps/modern-api-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "modern-api:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/modern-api-e2e/**/*.{js,ts}"]
          }
        }
